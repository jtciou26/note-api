{
    "name": "pubsub-bigquery-logger",
    "version": "1.0.0",
    "description": "Cloud Run function to process Pub/Sub messages and insert data into BigQuery",
    "main": "index.js",
    "scripts": {
        "start": "functions-framework --target=pubsubToBigQuery --signature-type=http",
        "dev": "NODE_ENV=development functions-framework --target=main --signature-type=http --port=8080",
        "test": "node test-local.js",
        "test:watch": "nodemon test-local.js"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^7.0.0",
        "@google-cloud/functions-framework": "^3.3.0"
    },
    "devDependencies": {
        "axios": "^1.6.0",
        "nodemon": "^3.0.1"
    },
    "engines": {
        "node": ">=18"
    }
}